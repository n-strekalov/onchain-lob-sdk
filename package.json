  },
  "peerDependencies": {
    "ws": "^8.17.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.10",
